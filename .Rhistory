library(stargazer)
library(dplyr)
# Making a subset of the data of the municipality of Utrecht
df_utrecht = data %>%
filter(grepl("Utrecht", munname))
df_utrecht_clean <- na.omit(df_utrecht[, c("lnpricem2", "distcbd", "shlandinfr","shopenspace", "popdens", "shforeign", "shlandres", "shyoungp", "shelderly")])
#converting a tibble into a dataframe and then making a table with stargazer with the descriptive statistics
df_utrecht_clean_df <- as.data.frame(df_utrecht_clean)
stargazer(df_utrecht_clean_df, type = "latex")
#Making a subset of the useful variables for the Netherlands
subdata_nl = data %>%
select(lnpricem2,distcbd, shlandinfr,shopenspace,popdens,shforeign,shlandres,shyoungp,shelderly)
#Converting a tibble into a dataframe and using stargazer to create a table with the descriptive statistics of the independent variables we will use the regression model
subdata_nl = as.data.frame(subdata_nl)
stargazer(subdata_nl, type = "latex")
stargazer(df_utrecht_clean, subdata_nl, type = "latex", column.labels = c("Utrecht", "The Netherlands"))
